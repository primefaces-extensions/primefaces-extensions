{
    "compilerOptions": {
        "target": "es2015",
        "moduleResolution": "node",
        "types": ["jquery", "primefaces"],
        "noEmit": true,
        "skipLibCheck": false,
        "skipDefaultLibCheck": true,
        "checkJs": true,
        "strict": true,
        "noImplicitAny": true,
        "useUnknownInCatchVariables": true,
        "strictPropertyInitialization": true
    },
    "include": ["."]
}