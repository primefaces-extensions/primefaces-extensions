<?xml version="1.0"?>
<faces-config xmlns="https://jakarta.ee/xml/ns/jakartaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-facesconfig_4_0.xsd"
              version="4.0">

    <name>primefaces_extensions</name>

    <ordering>
        <after>
            <name>primefaces</name>
        </after>
    </ordering>

    <application>
        <resource-handler>org.primefaces.extensions.application.PrimeFacesExtensionsResourceHandler</resource-handler>
        <system-event-listener>
            <system-event-listener-class>org.primefaces.extensions.application.PostConstructApplicationEventListener</system-event-listener-class>
            <system-event-class>jakarta.faces.event.PostConstructApplicationEvent</system-event-class>
            <source-class>jakarta.faces.application.Application</source-class>
        </system-event-listener>
    </application>

    <component>
        <component-type>org.primefaces.extensions.component.RemoteCommand</component-type>
        <component-class>org.primefaces.extensions.component.remotecommand.RemoteCommand</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.AssignableParameter</component-type>
        <component-class>org.primefaces.extensions.component.parameters.AssignableParameter</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.MethodParameter</component-type>
        <component-class>org.primefaces.extensions.component.parameters.MethodParameter</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.MasterDetail</component-type>
        <component-class>org.primefaces.extensions.component.masterdetail.MasterDetail</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.MasterDetailLevel</component-type>
        <component-class>org.primefaces.extensions.component.masterdetail.MasterDetailLevel</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Switch</component-type>
        <component-class>org.primefaces.extensions.component.switchcase.Switch</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Sheet</component-type>
        <component-class>org.primefaces.extensions.component.sheet.Sheet</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.SheetColumn</component-type>
        <component-class>org.primefaces.extensions.component.sheet.SheetColumn</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.SlideOut</component-type>
        <component-class>org.primefaces.extensions.component.slideout.SlideOut</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Case</component-type>
        <component-class>org.primefaces.extensions.component.switchcase.Case</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.DefaultCase</component-type>
        <component-class>org.primefaces.extensions.component.switchcase.DefaultCase</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.QRCode</component-type>
        <component-class>org.primefaces.extensions.component.qrcode.QRCode</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Timer</component-type>
        <component-class>org.primefaces.extensions.component.timer.Timer</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Speedtest</component-type>
        <component-class>org.primefaces.extensions.component.speedtest.Speedtest</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.TimeAgo</component-type>
        <component-class>org.primefaces.extensions.component.timeago.TimeAgo</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.MonacoEditorInline</component-type>
        <component-class>org.primefaces.extensions.component.monacoeditor.MonacoEditorInline</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.MonacoEditorFramed</component-type>
        <component-class>org.primefaces.extensions.component.monacoeditor.MonacoEditorFramed</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.MonacoDiffEditorInline</component-type>
        <component-class>org.primefaces.extensions.component.monacoeditor.MonacoDiffEditorInline</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.MonacoDiffEditorFramed</component-type>
        <component-class>org.primefaces.extensions.component.monacoeditor.MonacoDiffEditorFramed</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.OSMap</component-type>
        <component-class>org.primefaces.extensions.component.osmap.OSMap</component-class>
    </component>

    <converter>
        <converter-id>org.primefaces.extensions.converter.JsonConverter</converter-id>
        <converter-class>org.primefaces.extensions.converter.JsonConverter</converter-class>
    </converter>
    <converter>
        <converter-id>org.primefaces.extensions.converter.LocaleConverter</converter-id>
        <converter-class>org.primefaces.extensions.converter.LocaleConverter</converter-class>
    </converter>
    <converter>
        <converter-id>org.primefaces.extensions.converter.SanitizingConverter</converter-id>
        <converter-class>org.primefaces.extensions.converter.SanitizingConverter</converter-class>
    </converter>
    <converter>
        <converter-id>org.primefaces.extensions.converter.SanitizingLenientConverter</converter-id>
        <converter-class>org.primefaces.extensions.converter.SanitizingLenientConverter</converter-class>
    </converter>

    <render-kit>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.RemoteCommandRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.remotecommand.RemoteCommandRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.MasterDetailRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.masterdetail.MasterDetailRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.QRCodeRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.qrcode.QRCodeRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.TimerRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.timer.TimerRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.SheetRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.sheet.SheetRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.SlideOutRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.slideout.SlideOutRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.SpeedtestRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.speedtest.SpeedtestRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.TimeAgoRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.timeago.TimeAgoRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.MonacoEditorInlineRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.monacoeditor.MonacoEditorInlineRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.MonacoEditorFramedRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.monacoeditor.MonacoEditorFramedRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.MonacoDiffEditorInlineRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.monacoeditor.MonacoDiffEditorInlineRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.MonacoDiffEditorFramedRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.monacoeditor.MonacoDiffEditorFramedRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.OSMapRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.osmap.OSMapRenderer</renderer-class>
        </renderer>

        <client-behavior-renderer>
            <client-behavior-renderer-type>org.primefaces.extensions.behavior.JavascriptBehaviorRenderer</client-behavior-renderer-type>
            <client-behavior-renderer-class>org.primefaces.extensions.behavior.javascript.JavascriptBehaviorRenderer</client-behavior-renderer-class>
        </client-behavior-renderer>
    </render-kit>

</faces-config>