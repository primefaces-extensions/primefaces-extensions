<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

    <name>primefaces-extensions</name>

    <ordering>
        <after>
            <name>primefaces</name>
        </after>
    </ordering>

    <application>
        <resource-handler>org.primefaces.extensions.application.PrimeFacesExtensionsResourceHandler</resource-handler>
        <system-event-listener>
            <system-event-listener-class>org.primefaces.extensions.application.PostConstructApplicationEventListener</system-event-listener-class>
            <system-event-class>javax.faces.event.PostConstructApplicationEvent</system-event-class>
            <source-class>javax.faces.application.Application</source-class>
        </system-event-listener>
        <system-event-listener>
            <system-event-listener-class>org.primefaces.event.AutoUpdateComponentListener</system-event-listener-class>
            <system-event-class>javax.faces.event.PostAddToViewEvent</system-event-class>
            <source-class>org.primefaces.extensions.component.messages.Messages</source-class>
        </system-event-listener>
        <system-event-listener>
            <system-event-listener-class>org.primefaces.event.AutoUpdateComponentListener</system-event-listener-class>
            <system-event-class>javax.faces.event.PostAddToViewEvent</system-event-class>
            <source-class>org.primefaces.extensions.component.growl.Growl</source-class>
        </system-event-listener>
    </application>

    <behavior>
        <behavior-id>org.primefaces.extensions.behavior.JavascriptBehavior</behavior-id>
        <behavior-class>org.primefaces.extensions.behavior.javascript.JavascriptBehavior</behavior-class>
    </behavior>

    <component>
        <component-type>org.primefaces.extensions.component.CKEditor</component-type>
        <component-class>org.primefaces.extensions.component.ckeditor.CKEditor</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.CodeMirror</component-type>
        <component-class>org.primefaces.extensions.component.codemirror.CodeMirror</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Head</component-type>
        <component-class>org.primefaces.extensions.component.head.Head</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.ImageAreaSelect</component-type>
        <component-class>org.primefaces.extensions.component.imageareaselect.ImageAreaSelect</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.ImageRotateAndResize</component-type>
        <component-class>org.primefaces.extensions.component.imagerotateandresize.ImageRotateAndResize</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Message</component-type>
        <component-class>org.primefaces.extensions.component.message.Message</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Messages</component-type>
        <component-class>org.primefaces.extensions.component.messages.Messages</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.RemoteCommand</component-type>
        <component-class>org.primefaces.extensions.component.remotecommand.RemoteCommand</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.RemoteCommandParameter</component-type>
        <component-class>org.primefaces.extensions.component.remotecommand.RemoteCommandParameter</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.SaveState</component-type>
        <component-class>org.primefaces.extensions.component.savestate.SaveState</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Tooltip</component-type>
        <component-class>org.primefaces.extensions.component.tooltip.Tooltip</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Layout</component-type>
        <component-class>org.primefaces.extensions.component.layout.Layout</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.LayoutPane</component-type>
        <component-class>org.primefaces.extensions.component.layout.LayoutPane</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.AjaxStatus</component-type>
        <component-class>org.primefaces.extensions.component.ajaxstatus.AjaxStatus</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.MasterDetail</component-type>
        <component-class>org.primefaces.extensions.component.masterdetail.MasterDetail</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.MasterDetailLevel</component-type>
        <component-class>org.primefaces.extensions.component.masterdetail.MasterDetailLevel</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Growl</component-type>
        <component-class>org.primefaces.extensions.component.growl.Growl</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.KeyFilter</component-type>
        <component-class>org.primefaces.extensions.component.keyfilter.KeyFilter</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.BlockUI</component-type>
        <component-class>org.primefaces.extensions.component.blockui.BlockUI</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.Timeline</component-type>
        <component-class>org.primefaces.extensions.component.timeline.UITimeline</component-class>
    </component>
    <component>
        <component-type>org.primefaces.extensions.component.TimePicker</component-type>
        <component-class>org.primefaces.extensions.component.timepicker.TimePicker</component-class>
    </component>    
    <component>                        
        <component-type>org.primefaces.extensions.component.TriStateCheckbox</component-type>
        <component-class>org.primefaces.extensions.component.tristatecheckbox.TriStateCheckbox</component-class>
    </component>
    <component>                        
        <component-type>org.primefaces.extensions.component.TriStateManyCheckbox</component-type>
        <component-class>org.primefaces.extensions.component.tristatemanycheckbox.TriStateManyCheckbox</component-class>
    </component>

    <converter>
        <converter-id>org.primefaces.extensions.converter.JsonConverter</converter-id>
        <converter-class>org.primefaces.extensions.converter.JsonConverter</converter-class>
    </converter>
    <converter>
        <converter-id>org.primefaces.extensions.converter.LocaleConverter</converter-id>
        <converter-class>org.primefaces.extensions.converter.LocaleConverter</converter-class>
    </converter>

    <render-kit>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.CKEditorRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.ckeditor.CKEditorRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.CodeMirrorRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.codemirror.CodeMirrorRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.HeadRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.head.HeadRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.ImageAreaSelectRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.imageareaselect.ImageAreaSelectRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.RemoteCommandRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.remotecommand.RemoteCommandRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.ImageRotateAndResizeRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.imagerotateandresize.ImageRotateAndResizeRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.TooltipRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.tooltip.TooltipRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.LayoutRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.layout.LayoutRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.AjaxStatusRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.ajaxstatus.AjaxStatusRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.MessageRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.message.MessageRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.MessagesRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.messages.MessagesRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.MasterDetailRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.masterdetail.MasterDetailRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.GrowlRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.growl.GrowlRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.KeyFilterRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.keyfilter.KeyFilterRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.BlockUIRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.blockui.BlockUIRenderer</renderer-class>
        </renderer>        
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.TimelineRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.timeline.TimelineRenderer</renderer-class>
        </renderer>        
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.TimePickerRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.timepicker.TimePickerRenderer</renderer-class>
        </renderer>   
        
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.TriStateCheckboxRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.tristatecheckbox.TriStateCheckboxRenderer</renderer-class>
        </renderer>
        
        <renderer>
            <component-family>org.primefaces.extensions.component</component-family>
            <renderer-type>org.primefaces.extensions.component.TriStateManyCheckboxRenderer</renderer-type>
            <renderer-class>org.primefaces.extensions.component.tristatemanycheckbox.TriStateManyCheckboxRenderer</renderer-class>
        </renderer>

        <client-behavior-renderer>
            <client-behavior-renderer-type>org.primefaces.extensions.behavior.JavascriptBehaviorRenderer</client-behavior-renderer-type>
            <client-behavior-renderer-class>org.primefaces.extensions.behavior.javascript.JavascriptBehaviorRenderer</client-behavior-renderer-class>
        </client-behavior-renderer>
    </render-kit>

</faces-config>
