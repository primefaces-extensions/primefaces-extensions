<?xml version="1.0"?>
<facelet-taglib version="4.0"
                xmlns="https://jakarta.ee/xml/ns/jakartaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-facelettaglibrary_4_0.xsd">

    <namespace>primefaces.extensions.showcase</namespace>

    <function>
        <function-name>getFileContent</function-name>
        <function-class>org.primefaces.extensions.showcase.util.ShowcaseUtil</function-class>
        <function-signature>java.lang.String getFileContent(java.lang.String)</function-signature>
    </function>

    <tag>
        <tag-name>convertOrder</tag-name>
        <converter>
            <converter-id>org.primefaces.extensions.showcase.converter.OrderConverter</converter-id>
        </converter>
        <attribute>
            <name>events</name>
            <required>true</required>
            <type>java.util.List</type>
        </attribute>
    </tag>

</facelet-taglib>
